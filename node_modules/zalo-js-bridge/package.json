{
  "name": "zalo-js-bridge",
  "description": "A Javascript Framework for WebApp",
  "version": "1.13.2",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "keywords": [],
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "compile": "rm -rf ./lib && tsc && copyfiles -f ./src/main/*.js ./lib",
    "lint": "tslint -c ./tslint.json -p ./tsconfig.tslint.json",
    "test": "TS_NODE_PROJECT=tsconfig.test.json TS_NODE_CACHE=false mocha -r ts-node/register test/*.test.ts",
    "docs": "npm rum compile && jsdoc -c jsdoc.js",
    "release": "semantic-release",
    "use:npmReadme": "mv 'README.md' 'git.README.md' && mv 'npm.README.md' 'README.md'",
    "use:gitReadme": "mv 'README.md' 'npm.README.md' && mv 'git.README.md' 'README.md'",
    "prepublishOnly": "echo $npm_package_version > version.txt && npm run compile && npm run build && run-s build use:npmReadme",
    "postpublish": "npm run use:gitReadme && npm run docs && git add -f docs && git commit -m 'docs: update docs'"
  },
  "dependencies": {
    "js-cookie": "^2.2.1",
    "query-string-encode": "^1.0.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.0.7",
    "@types/assert": "^1.5.1",
    "@types/bluebird": "^3.5.32",
    "@types/debug": "^4.1.5",
    "@types/events": "^3.0.0",
    "@types/js-cookie": "^2.2.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.14.10",
    "@types/object-assign": "^4.0.30",
    "better-docs": "^2.3.2",
    "bluebird": "^3.7.2",
    "browserify": "^16.5.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.0",
    "copyfiles": "^2.3.0",
    "debug": "^4.3.1",
    "docdash-custom": "^1.2.0",
    "dotenv-webpack": "^6.0.0",
    "ejs": "^3.1.5",
    "events": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.0.0",
    "html-webpack-injector": "^1.1.4",
    "html-webpack-plugin": "^4.5.0",
    "jsdoc": "^3.6.6",
    "kida-docs": "^1.0.3",
    "mkdirp": "^1.0.4",
    "mocha": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "object-assign": "^4.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "semantic-release": "^17.3.0",
    "sha.js": "^2.4.11",
    "string-replace-webpack-plugin": "^0.1.3",
    "sync-request": "^6.1.0",
    "terser": "^4.8.0",
    "ts-loader": "^7.0.5",
    "ts-mockito": "^2.6.1",
    "ts-node": "^8.10.2",
    "tsc-webpack-plugin": "^0.0.4",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
